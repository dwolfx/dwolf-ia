{
  "name": "dwolf-ia",
  "displayName": "my-ia-helper",
  "description": "Plugin de VS Code que usa IA pra codar de gra√ßa",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deepseek",
          "title": "DeepSeek",
          "icon": "media/chat-wolf-icon.svg"
        }
      ]
    },
    "views": {
      "deepseek": [
        {
          "id": "deepseekChat",
          "name": "Chat",
          "icon": "media/chat-wolf-icon.svg"
        }
      ]
    }
